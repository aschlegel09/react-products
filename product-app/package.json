{
  "name": "react-products",
  "version": "1.0.0",
  "repository": "https://github.com/aschlegel09/react-products.git",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Andrew Schlegel <aschlegel09@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-server": "^0.7.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.1",
    "morgan": "^1.9.1",
    "react": "^16.7.0",
    "react-chat-widget": "^2.1.4-1",
    "react-dom": "^16.7.0",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^4.0.1",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "utils": "^0.3.1"
  }
}
